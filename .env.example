# Digital VisionWorks Environment Variables
# Copy this file to .env and fill in the values

# Google Gemini AI API Key (for AI features)
# Get your API key from: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Analytics
# VITE_GA_TRACKING_ID=G-XXXXXXXXXX

# Optional: Contact Form Backend (for future email integration)
# VITE_CONTACT_FORM_ENDPOINT=https://your-api-endpoint.com/contact

# Environment (development | production)
NODE_ENV=development
